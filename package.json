{
  "name": "pip-webui2-themes",
  "version": "1.1.22",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build pip-webui2-themes && npm run build:lib-styles",
    "build:lib-styles": "scss-bundle -c ./projects/pip-webui2-themes/scss-bundle.styles.json && scss-bundle -c ./projects/pip-webui2-themes/scss-bundle.variables.json",
    "test": "ng test",
    "test:lib": "ng test pip-webui2-themes",
    "lint": "ng lint",
    "lint:lib": "ng lint pip-webui2-themes",
    "e2e": "ng e2e",
    "package": "npm run build:lib && (for %I in (LICENSE README.md CHANGELOG.md) do copy .\\%I .\\dist\\pip-webui2-themes) && cd dist/pip-webui2-themes && npm pack",
    "deploy": "ng build --configuration production --base-href https://pip-webui2.github.io/pip-webui2-themes/ && ngh --dir dist/pip-webui2-themes-app"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^14.2.10",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.10",
    "@angular/compiler": "^14.2.10",
    "@angular/core": "^14.2.10",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "^14.2.10",
    "@angular/localize": "^14.2.10",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.10",
    "@angular/platform-browser-dynamic": "^14.2.10",
    "@angular/router": "^14.2.10",
    "core-js": "^2.5.7",
    "rxjs": "~6.6.7",
    "scss-bundle": "^3.1.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular/cli": "^14.2.9",
    "@angular/compiler-cli": "^14.2.10",
    "@angular/language-service": "^14.2.10",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lodash": "^4.17.11",
    "ng-packagr": "^14.2.2",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}
