{
  "name": "pip-webui-themes-ngx",
  "version": "14.0.6",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build pip-webui-themes-ngx --configuration production && cd ./dist/pip-webui-themes-ngx && npm pack",
    "format:check": "prettier --config .\\.prettierrc --list-different \"{projects,src/{app,environments,assets}}/**/*{.ts,.js,.html,.json,.css,.scss}\"",
    "format:fix": "prettier --config .\\.prettierrc --write \"{projects,src/{app,environments,assets}}/**/*{.ts,.js,.html,.json,.css,.scss}\"",
    "test": "ng test",
    "test:lib": "ng test pip-webui-themes-ngx --browsers=ChromeHeadlessNoSandbox --watch=false --code-coverage=false",
    "lint": "ng lint",
    "lint:lib": "ng lint pip-webui-themes-ngx",
    "e2e": "ng e2e",
    "package": "npm run build:lib && (for %I in (LICENSE README.md CHANGELOG.md) do copy .\\%I .\\dist\\pip-webui-themes-ngx) && cd dist/pip-webui-themes-ngx && npm pack",
    "deploy": "ng build --configuration production --base-href https://pip-webui2.github.io/pip-webui-themes-ngx/ && ngh --dir dist/pip-webui-themes-ngx-app"
  },
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:fix"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@angular/animations": "^14.2.10",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.10",
    "@angular/compiler": "^14.2.10",
    "@angular/core": "^14.2.10",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "^14.2.10",
    "@angular/localize": "^14.2.10",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.10",
    "@angular/platform-browser-dynamic": "^14.2.10",
    "@angular/router": "^14.2.10",
    "@ngneat/transloco": "^4.1.1",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "^14.2.9",
    "@angular/compiler-cli": "^14.2.10",
    "@angular/language-service": "^14.2.10",
    "@types/jasmine": "~3.8.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^14.18.33",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "angular-cli-ghpages": "^1.0.5",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jasmine-core": "~3.8.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^13.0.3",
    "ng-packagr": "^14.2.2",
    "prettier": "^2.8.0",
    "protractor": "~7.0.0",
    "typescript": "~4.7.4"
  }
}
