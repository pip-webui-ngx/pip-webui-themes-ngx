{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es2020",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2019",
      "dom"
    ],
    "paths": {
      "pip-webui-themes-ngx": [
        "dist/pip-webui-themes-ngx"
      ],
      "pip-webui-themes-ngx/*": [
        "dist/pip-webui-themes-ngx/*"
      ]
    }
  }
}
